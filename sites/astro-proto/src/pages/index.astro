import '../scss/style.scss'
import '../scss/prism-vsc-dark-plus.css'

import { Prism } from '@astrojs/prism';

import Layout from "../layouts/Layout.astro"
import BasicExample from '../../../../packages/astro-splide/examples/components/BasicExample.astro'
import AutoplayExample from '../../../../packages/astro-splide/examples/components/AutoplayExample.astro'

const pageHead = {
  title: 'Title of the page',
  description: "Descriptions of this page",
}

const code = `---
const foo = 'bar';
---
<Prism lang="astro" code={code} />
`

---

<Layout pageHead={pageHead}>

  <section class="webtools-container">

    <h1>Astro Page example using Webtools </h1>

    <BasicExample/>
    <Prism lang="astro" code={code} />


    <AutoplayExample/>

  </section>

</Layout>
